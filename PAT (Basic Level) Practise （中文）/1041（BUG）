# include<iostream>
# include<string>
using namespace std;

struct Student
{
    string id;
    int seat;
}; 

int main()
{
    int num1;
    cin>>num1;
    Student *stu = new Student[num1+1];
    for(int i = 1; i<= num1; i++)
    {
        string temp1;
        int temp2;
        cin>>temp1>>temp2;
        cin>>stu[temp2].seat;
        stu[temp2].id = temp1;
    }
    int num2;
    for(int i =1;i<= num2;i++)
    {
        int temp;
        cin>>temp;
        cout<<stu[temp].id<<" "<<stu[temp].seat;
        if(i != num2)
            cout<<endl;
    }
    system("pause");
    return 0;
    
}
